package android.app1.findfriends.models;

public class Contact {

	private String firstName;
	private String lastName;
	private String userName;
	private String emailAddress;
	private String password;
	private String phoneNumber;
	private String latitiude;
	private String longitude;
	private String tracked;
	private String showing_others;
	private String showing_friends;
	private byte[] image;
	private String time_stamp;

	
    public Contact(){
    	this.firstName = "";
		this.lastName = "";
		this.userName = "";
		this.phoneNumber = "";
		this.latitiude = "0.0";
		this.longitude = "0.0";
		this.tracked = "F";
		this.showing_others = "T";
		this.showing_friends = "T";
		this.setImage(new byte[0]);
    }
	public Contact(String firstName, String lastName, String phoneNumber) {
		this.firstName = firstName;
		this.lastName = lastName;
		this.userName = "";
		this.phoneNumber = phoneNumber;
		this.latitiude = "0.0";
		this.longitude = "0.0";
		this.tracked = "F";
		this.showing_others = "T";
		this.showing_friends = "T";
		
	}

	public String getFirstName() {
		return firstName;
	}

	public void setFirstName(String firstName) {
		this.firstName = firstName;
	}

	public String getLastName() {
		return lastName;
	}

	public void setLastName(String lastName) {
		this.lastName = lastName;
	}

	public String getEmailAddress() {
		return emailAddress;
	}

	public void setEmailAddress(String emailAddress) {
		this.emailAddress = emailAddress;
	}

	public String getPassword() {
		return password;
	}

	public void setPassword(String password) {
		this.password = password;
	}

	public String getPhoneNumber() {
		return phoneNumber;
	}

	public void setPhoneNumber(String phoneNumber) {
		this.phoneNumber = phoneNumber;
	}

	@Override
	public String toString() {
		return this.firstName + " " +this.lastName + " " + emailAddress;
	}

	public String getLatitiude() {
		return latitiude;
	}

	public void setLatitiude(String latitiude) {
		this.latitiude = latitiude;
	}

	public String getLongitude() {
		return longitude;
	}

	public void setLongitude(String longitude) {
		this.longitude = longitude;
	}

	public String getTracked() {
		return tracked;
	}

	public void setTracked(String tracked) {
		this.tracked = tracked;
	}
	public String getShowing_others() {
		return showing_others;
	}
	public void setShowing_others(String showing_others) {
		this.showing_others = showing_others;
	}
	public String getShowing_friends() {
		return showing_friends;
	}
	public void setShowing_friends(String showing_friends) {
		this.showing_friends = showing_friends;
	}
	public byte[] getImage() {
		return image;
	}
	public void setImage(byte[] image) {
		this.image = image;
	}
	/**
	 * @return the userName
	 */
	public String getUserName() {
		return userName;
	}
	/**
	 * @param userName the userName to set
	 */
	public void setUserName(String userName) {
		this.userName = userName;
	}
	/**
	 * @return the time_stamp
	 */
	public String getTime_stamp() {
		return time_stamp;
	}
	/**
	 * @param time_stamp the time_stamp to set
	 */
	public void setTime_stamp(String time_stamp) {
		this.time_stamp = time_stamp;
	}
	

	
	
}
